<p id="notice"><%= notice %></p>

<h1>Sectores</h1>

<table>
  <thead>
    <tr>
      <th>Código de Municipio</th>
      <th>Código de Sector</th>
      <th>Descripción</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectors.each do |sector| %>
      <tr>
        <td><%= sector.Municipality.num_municip %></td>
        <td><%= sector.num_sector %></td>
        <td><%= sector.description %></td>
        <td><%= link_to 'Mostrar', sector %></td>
        <td><%= link_to 'Modificar', edit_sector_path(sector) %></td>
        <td><%= link_to 'Eliminar', sector, method: :delete, data: { confirm: 'Esta Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Sector', new_sector_path %>
